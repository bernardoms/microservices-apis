{
	"request": {
		"method": "GET",
		"urlPathPattern": "/v1/campaigns",
		"queryParameters" : {
			"team_id" : {
				"matches" : ".*"
			},
			"offset" : {
				"equalTo" : "0"
			},
			"limit" : {
				"equalTo" : "1"
			}
		}
	},
	"response": {
		"status": 200,
		"bodyFileName": "campaigns-page1.json",
		"headers": {
			"Content-Type": "application/json; charset=utf-8"
		}
	}
}
